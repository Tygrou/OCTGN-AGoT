	<!-- Attachment Card template -->
	<template src="proxy/proxyImages/attachmentBackground.jpg" default="False">
		<matches>
			<match name="Type" value="Attachment" />
		</matches>
		
		<overlayblocks>
			<!-- Title Bar selection -->
			<conditional>
				<if property="House" value="Stark">
					<link block="starkBar" /> </if>
			</conditional>
			<conditional>
				<if property="House" value="Baratheon">
					<link block="baratheonBar" /> </if>
			</conditional>
			<conditional>
				<if property="House" value="Greyjoy">
					<link block="greyjoyBar" /> </if>
			</conditional>
			<conditional>
				<if property="House" value="Lannister">
					<link block="lannisterBar" /> </if>
			</conditional>
			<conditional>
				<if property="House" value="Martell">
					<link block="martellBar" /> </if>
			</conditional>
			<conditional>
				<if property="House" value="Targaryen">
					<link block="targaryenBar" /> </if>
			</conditional>
			
			<!-- Unique test -->
			<conditional>
				<if property="Unique" value="Yes">
					<link block="uniqueYes" /> </if>
			</conditional>
			<conditional>
				<if property="Unique" value="Infinity">
					<link block="uniqueInfinity" /> </if>
			</conditional>
			
			<!-- House affiliation selection -->
			<conditional>
				<if property="House" contains="Stark">
					<link block="shieldStark" /> </if>
			</conditional>
			<conditional>
				<if property="House" contains="Baratheon">
					<link block="shieldBaratheon" /> </if>
			</conditional>
			<conditional>
				<if property="House" contains="Lannister">
					<link block="shieldLannister" /> </if>
			</conditional>
			<conditional>
				<if property="House" contains="Greyjoy">
					<link block="shieldGreyjoy" /> </if>
			</conditional>
			<conditional>
				<if property="House" contains="Martell">
					<link block="shieldMartell" /> </if>
			</conditional>
			<conditional>
				<if property="House" contains="Targaryen">
					<link block="shieldTargaryen" /> </if>
			</conditional>
			<conditional>
				<if property="House" contains="Neutral">
					<link block="shieldNeutral" /> </if>
			</conditional>
			
			<!-- Gold/Intrigue/Initiative -->
			<conditional>
				<if property="GoldIncome" value="">
					<link block="dummyOverlay" /> </if>
				<else>
					<link block="cardGold" /> </else>
			</conditional>
			<conditional>
				<if property="Influence" value="">
					<link block="dummyOverlay" /> </if>
				<else>
					<link block="cardInfluence" /> </else>
			</conditional>
			<conditional>
				<if property="Initiative" value="">
					<link block="dummyOverlay" /> </if>
				<else>
					<link block="cardInitiative" /> </else>
			</conditional>
			<link block="dummyOverlay" />			
		</overlayblocks>
		
		<textblocks>
			<conditional>
				<if property="Cost" contains="s">
					<link block="cardCost_shadows">
						<property name="Cost" />
					</link> </if>
				<else>
					<link block="cardCost">
						<property name="Cost" />
					</link> </else>
			</conditional>
			
			<!-- Gold/Intrigue/Initiative -->
			<conditional>
				<if property="GoldIncome" value="">
					<link block="dummyOverlay" /> </if>
				<else>
					<link block="textGold" >
						<property name="GoldIncome" />
					</link> </else>
			</conditional>
			<conditional>
				<if property="Influence" value="">
					<link block="dummyOverlay" /> </if>
				<else>
					<link block="textInfluence" >
						<property name="Influence" />
					</link> </else>
			</conditional>
			<conditional>
				<if property="Initiative" value="">
					<link block="dummyOverlay" /> </if>
				<else>
					<link block="textInitiative" >
						<property name="Initiative" />
					</link> </else>
			</conditional>
			
			<link block="dummyOverlay" />
			<link block="cardName">
				<property name="Name" />
			</link>
			<link block="cardTraits">
				<property name="Traits" />
			</link>
			<link block="cardKeywords">
				<property name="Keywords" />
			</link>
			<link block="cardText">
				<property name="Text" />
			</link>
		</textblocks>
	</template>
	
